{
  "name": "trinil-svelte",
  "version": "1.3.7",
  "description": "SVG icon library for Svelte with locked stroke styles",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "svelte": "dist/index.js",
  "files": [
    "dist",
    "README.md",
    "ICONS.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "svelte": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "peerDependencies": {
    "svelte": ">=3 || >=4 || >=5"
  },
  "devDependencies": {
    "svelte": "^5.0.0",
    "typescript": "^5.3.3",
    "tsup": "^8.0.2"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "clean": "rm -rf dist && find src/icons -type f -name '*.ts' -delete"
  }
}
