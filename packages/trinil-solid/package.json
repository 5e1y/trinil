{
  "name": "trinil-solid",
  "version": "1.3.0",
  "description": "SVG icon library for SolidJS with locked stroke styles",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "ICONS.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "solid": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "peerDependencies": {
    "solid-js": ">=1.0.0"
  },
  "devDependencies": {
    "solid-js": "^1.8.0",
    "typescript": "^5.3.3",
    "tsup": "^8.0.2"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "clean": "rm -rf dist && find src/icons -type f -name '*.tsx' -delete"
  }
}
